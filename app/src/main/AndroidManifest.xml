<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.heima.tabview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.heima.tabview.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.heima.tabview.QuickStartActivity"></activity>
        <activity android:name="com.heima.tabview.CustomInXmlActivity"></activity>
        <activity android:name="com.heima.tabview.CustomInJavaActivity"></activity>
        <activity android:name="com.heima.tabview.UseInFragment"></activity>

    </application>

</manifest>